# OpenRouter / Model Routing
OPENROUTER_API_KEY=
OPENROUTER_BASE=https://openrouter.ai/api/v1
OPENROUTER_POLICY=./openrouter-policy.yml
OPENROUTER_MODEL=openai/gpt-4o-mini
HTTP_REFERER=https://local.dev
X_TITLE=Autonomous Factory Advice

# Factory Runners / Orchestrator
FACTORY_PROJECT_NAME=autonomous-factory
FACTORY_OUT_DIR=.factory-out
FACTORY_DIFF_MAX_LINES=500
FACTORY_STRICT_CI=true

# Budgets / Gates
PERF_P95_MS=50
MUTATION_SCORE_MIN=0.7
COVERAGE_MIN=0.9
A11Y_STANDARD=AA

# Logging / Telemetry
FACTORY_TRACE_LEVEL=info
FACTORY_REDACT_SECRETS=true
